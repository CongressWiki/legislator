{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@type/*": [
        "src/types/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@static/*": [
        "static/*"
      ],
      "@images/*": [
        "static/images/*"
      ],
      "@constants": [
        "src/constants"
      ]
    },
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": true,
    // "checkJs": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    // "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    // "skipLibCheck": true
    "sourceMap": true
  },
  "include": [
    "src"
  ]
}