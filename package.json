{
  "name": "legislator",
  "description": "",
  "private": true,
  "version": "0.1.0",
  "author": "Ryan Parker",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=6144 gatsby build",
    "dev": "NODE_OPTIONS=--max-old-space-size=6144 gatsby develop --verbose",
    "dev:lan": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "prettier --check src && eslint src",
    "fix": "prettier --write src && eslint --fix src",
    "chromatic": "chromatic --project-token CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "clean:ports": "lsof -nti:8000 | xargs kill -9",
    "clean:svg": "svgo static/images/*.svg --pretty --recursive",
    "clean": "yarn clean:ports && yarn gatsby clean",
    "storybook:setup": "cp -r ./public/page-data/sq/d ./public/static",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E9MUWJRG8LV8 --paths \"/*\"",
    "deployToProduction": "gatsby-plugin-s3 deploy --yes && yarn invalidate",
    "cleanBuildDeploy": "yarn clean && yarn build && yarn deployToProduction"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@auth0/auth0-react": "^1.8.0",
    "@gitgraph/core": "^1.5.0",
    "@svgr/webpack": "^5.5.0",
    "auth0-js": "^9.16.4",
    "babel-plugin-styled-components": "^1.13.2",
    "d3": "^7.0.3",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.14.00",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^3.14.0",
    "gatsby-plugin-google-gtag": "^3.14.0",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-source-graphql": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "isomorphic-fetch": "^3.0.0",
    "jimp": "^0.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "remove-accents": "^0.4.2",
    "styled-components": "^5.3.1",
    "subscriptions-transport-ws": "^0.9.19",
    "wasm-twitter-card": "^0.3.0",
    "ws": "^7.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@svgr/cli": "^5.5.0",
    "@types/auth0-js": "^9.14.5",
    "@types/d3": "^7.0.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.173",
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.14",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-remove-graphql-queries": "3.14.0",
    "babel-preset-react-app": "^10.0.0",
    "chromatic": "^5.10.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.38.0",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-config-xo-typescript": "^0.44.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "lodash": "^4.17.21",
    "prettier": "2.4.1",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "gatsby"
  ]
}
