{
  "name": "legislator",
  "description": "",
  "private": true,
  "version": "0.1.0",
  "author": "Ryan Parker",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=6144 gatsby build",
    "dev": "NODE_OPTIONS=--max-old-space-size=6144 gatsby develop",
    "dev:lan": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "xo src",
    "fix": "xo src --fix",
    "clean:svg": "svgo static/images/*.svg static/images/**/*.svg --pretty --recursive",
    "clean": "gatsby clean",
    "storybook:setup": "cp -r ./public/page-data/sq/d ./public/static",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "NODE_ENV=production build-storybook --no-dll  -s public",
    "deployToProduction": "gatsby-plugin-s3 deploy --yes && aws cloudfront create-invalidation --distribution-id E126LGFP9KWM65 --paths \"/*\"",
    "cleanBuildDeploy": "yarn clean && yarn build && yarn deployToProduction"
  },
  "xo": {
    "prettier": true,
    "envs": [
      "browser",
      "es2020"
    ],
    "extends": [
      "xo-react"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "pascalCase"
        }
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@svgr/webpack": "^5.5.0",
    "babel-plugin-styled-components": "^1.12.0",
    "d3": "^6.6.1",
    "framer-motion": "^4.0.3",
    "gatsby": "^3.1.2",
    "gatsby-image": "^3.1.0",
    "gatsby-plugin-canonical-urls": "^3.1.0",
    "gatsby-plugin-google-gtag": "^3.1.0",
    "gatsby-plugin-image": "^1.1.2",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^3.1.2",
    "gatsby-plugin-sitemap": "^3.1.0",
    "gatsby-plugin-styled-components": "^4.1.0",
    "gatsby-plugin-svgr": "3.0.0-beta.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-source-graphql": "^3.1.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jimp": "^0.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "remove-accents": "^0.4.2",
    "styled-components": "^5.2.1",
    "subscriptions-transport-ws": "^0.9.18",
    "wasm-twitter-card": "^0.3.0",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/auth0-js": "^9.14.2",
    "@types/d3": "^6.3.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.168",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.9",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-remove-graphql-queries": "3.1.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.23.0",
    "eslint-config-xo": "^0.35.0",
    "eslint-config-xo-react": "^0.24.0",
    "eslint-config-xo-typescript": "^0.38.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lodash": "^4.17.21",
    "prettier": "2.2.1",
    "stylelint-config-xo": "^0.20.0",
    "typescript": "^4.2.3",
    "xo": "^0.38.2"
  },
  "keywords": [
    "gatsby"
  ]
}